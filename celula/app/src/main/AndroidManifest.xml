        <activity
            android:name=".FrequencyPkg.Reports"
            android:label="@string/title_activity_reports"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrequencyPkg.Frequency"
            android:label="@string/title_activity_frequency"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrequencyPkg.CellFrequencyReport"
            android:label="@string/title_activity_cell_frequency_report"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrequencyPkg.ServiceFrequencyReport"
            android:label="@string/title_activity_service_frequency_report"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrequencyPkg.CellFrequencyReportList"
            android:label="@string/title_activity_cell_frequency_report_list"
            android:theme="@style/AppTheme.NoActionBar" />
