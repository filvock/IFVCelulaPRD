            android:label="@string/title_activity_edit_frequency"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrequencyPkg.FrequencyEditCell"
            android:label="@string/title_activity_frequency_edit_cell"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FrequencyPkg.FrequencyEditService"
            android:label="@string/title_activity_frequency_edit_service"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
