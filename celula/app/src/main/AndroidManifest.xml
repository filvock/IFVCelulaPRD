            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".MembersPkg.MemberDetails"
            android:label="@string/title_activity_member_details"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".MembersPkg.AddMember"
            android:label="@string/title_activity_add_member"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".FrequencyPkg.CellFrequency"
            android:label="@string/title_activity_cell_report_data"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".FrequencyPkg.ServiceFrequency"
            android:label="@string/title_activity_service_frequency"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".SuccessLaderPkg.EscadaSucesso"
            android:label="@string/title_activity_escada_sucesso"
            android:theme="@style/AppTheme.NoActionBar" />
