<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".FrequencyPkg.CellFrequency"
    tools:showIn="@layout/frequency_1_cell_activity"
    android:background="@mipmap/ifvlogo">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cell_frequency"
        android:id="@+id/CellReportDataFrequency"
        android:onClick="CellReportDataFrequency"
        android:layout_marginTop="54dp"
        android:layout_below="@+id/textView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/CellReportDataOk"
        android:layout_alignEnd="@+id/CellReportDataOk" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cell_offer"
        android:id="@+id/CellReportDataOffer"
        android:layout_alignParentStart="true"
        android:onClick="CellReportDataOffer"
        android:layout_below="@+id/CellReportDataFrequency"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/CellReportDataOk"
        android:layout_alignEnd="@+id/CellReportDataOk" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cell_nocell"
        android:id="@+id/CellReportDataNoCell"
        android:layout_alignParentStart="true"
        android:onClick="CellReportDataNoCell"
        android:layout_below="@+id/CellReportDataOffer"
        android:layout_alignParentLeft="true"
        android:layout_alignRight="@+id/CellReportDataOk"
        android:layout_alignEnd="@+id/CellReportDataOk" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_back"
        android:id="@+id/CellReportDataOk"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="59dp"
        android:onClick="CellReportDataOk" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/cell_date2"
        android:id="@+id/textView12"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/cellDateString"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/CellReportDataFrequency"
        android:layout_alignEnd="@+id/CellReportDataFrequency"
        android:layout_toEndOf="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_toRightOf="@+id/textView12" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cell_date3"
        android:id="@+id/button4"
        android:layout_below="@+id/CellReportDataNoCell"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/cellDateString"
        android:layout_alignEnd="@+id/cellDateString"
        android:onClick="CellReportNewDate" />
</RelativeLayout>
